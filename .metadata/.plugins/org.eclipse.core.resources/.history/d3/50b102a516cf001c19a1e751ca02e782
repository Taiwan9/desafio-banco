package conta;

import java.util.List;

public class Banco {
	private String nome;
	private List<Conta> contas;
	
	public Banco() {}

	public Banco(String nome) {
		super();
		this.nome = nome;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}
	
	

}
